@import '/template/template.wxss';

.user_container .tips {
  padding-left: 24rpx;
}
.avatar_wrapper .color_g {
  background: #38CDED;
}
.user_container {
  height: 1100rpx;
  width: 750rpx;
  padding: 0 0 134rpx 0;
  display: flex;
  justify-content: center;
}
.user_item {
  box-sizing: border-box;
  padding: 0 20rpx;
  width: 702rpx;
  height: 160rpx;
  background: rgba(255,255,255,1);
  box-shadow: 0rpx 3rpx 11rpx 5rpx rgba(0, 0, 0, 0.14);
  border-radius: 9rpx;
  margin: 0 auto 22rpx;
}
.user_msg {
  flex: 1;
}
.tips {
  padding: 15rpx 0;
}
.avatar {
  width: 120rpx;
  height: 120rpx;
}
.avatar_wrapper {
  position: relative;
}
.enroll_text {
  position: absolute;
  left: 5rpx;
  bottom: 0;
  width: 110rpx;
  text-align: center;
  line-height: 30rpx;
  color: #fff;
  height: 30rpx;
  background: #FF8A8A;
  border-radius: 8rpx;
}
.user_label>view {
  margin: 20rpx 20rpx 0 0;
  line-height: 40rpx;
  height: 40rpx;
  box-sizing: border-box;
  padding: 0 10rpx;
  color: #FFA1A3;
  font-size: 24rpx;
  background: rgba(255,249,251,1);
  border: 1rpx solid rgba(255,161,163,1);
  border-radius: 8rpx;
}
.heart_wrapper image {
  width: 46rpx;
  height: 46rpx;
}
.height {
  height: 800rpx;
  background: palegoldenrod;
}
.no_more {
  text-align: center
}
.search_wrapper {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 110rpx;
  border-bottom: 1rpx solid rbga(153, 153, 153);
}
.search_btn {
  display: flex;
  justify-content: center;
  height: 70rpx;
  text-align: center;
  background: rgba(238,238,238,1);
  width: 702rpx;
  border-radius: 44rpx;
}