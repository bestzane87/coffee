.top_container {
  border-bottom: 4rpx solid #f2f2f2;
  padding: 30rpx 0;
  font-size: 30rpx;
  position: fixed;
  top: 90rpx;
  left: 0;
  height: 100rpx;
  box-sizing: border-box;
  background: white;
  width: 100%;
}
.padding_t {
  padding-top: 190rpx;
}
.top_bar {
  display: flex;
  padding: 0 24rpx;
  height: 100%;
}
.city {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 24rpx;
  height: 90rpx;
  align-items: center;
  justify-content: space-between;
  background: white;
  z-index: 999;
}
.city text {
  font-size: 34rpx;
  margin-left: 15rpx;
}
image {
  vertical-align: middle;
}
.icon_location {
  width: 34rpx;
  height: 34rpx;
}
.checked {
  color: #A37DF6;
}
.arrow_top {
  width: 18rpx;
  height: 18rpx;
  margin: 0 40rpx 0 10rpx;
}
.type_title {
  display: flex;
  align-items: center;
}
.type_title:nth-child(4) {
  flex: 1;
  display: flex;
  justify-content: flex-end;
}
.type_title:nth-child(4) image {
  margin-right: 0;
}
.queue_item {
  position: absolute;
  top: 100rpx;
  background: #fff;
  width: 100%;
  z-index: 999999999999;
}
.list_item {
  height: 80rpx;
  padding: 0 24rpx;
  background:rgba(250,250,250,1);
  line-height: 80rpx;
  border-bottom: 1rpx solid rgba(153, 153, 153, .3);
}
.act_item {
  display: flex;
  padding: 24rpx;
  border-bottom: 22rpx solid #eee;
}
.act_item_name {
  font-size: 32rpx;
  width: 412rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
}
.no_data, .no_activity {
  text-align: center;
  color: #999;
  height: 80rpx;
  line-height: 80rpx;
}
.no_activity {
  height: 900rpx;
  line-height: 900rpx;
}
.detail_wrapper {
  justify-content: space-between;
  padding-left: 22rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
}
.act_item_person {
  font-size: 24rpx;
}
.bottom_con {
  width: 100%;
  display: flex;
  font-size: 22rpx;
  justify-content: space-between;
  align-items: flex-start;
}
.bottom_con view:nth-child(1) {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 300rpx;
  color: #999;
}
.act_item_cost {
  flex: 1;
  font-size: 22rpx;
  color: #FFA200;
  text-align: right;
}
.img_wrapper image {
  width:260rpx;
  height:179rpx;
  border-radius:8rpx;
}
.act_item_person {
  display: flex;
  align-items: flex-start;
  justify-content: space-between;
}
.color_l {
  color:rgba(153,153,153,1);
}