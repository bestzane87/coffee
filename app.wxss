view {
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color: #333;
}
.wrapper {
  display: flex;
  justify-content: center;
  align-items: center;
}
.arrow-r {
  height: 30rpx;
  width: 16rpx;
}
.avatar {
  height: 128rpx;
  width: 128rpx;
  border-radius: 64rpx;
}
/* window的样式 */
.container-d {
  padding: 30rpx 24rpx;
}
.section {
  padding: 30rpx;
  box-shadow: 0 0 10px rgba(0,0,0,0.14);
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}
.section-h {
  box-shadow: 0 0 10px rgba(0,0,0,0.14);
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  padding: 0 30rpx 24rpx 30rpx;
}
.section-title {
  height: 90rpx;
  border-bottom: 1rpx solid #eee;
  line-height: 90rpx;
  font-size: 17px;
  margin-bottom: 22rpx;
}
.makesure {
  width: 666rpx;
  height: 96rpx;
  background: linear-gradient(146deg,rgba(116,110,243,1),rgba(168,107,255,1));
  border-radius: 48rpx;
  color: white;
  margin: 0 auto;
}
image {
  vertical-align: bottom;
}
.flexible {
  display: flex;
  align-items: center;
}
.arrow_r {
  width: 16rpx;
  height: 30rpx;
}
.fs_34 {
  font-size: 34rpx;
}
.fs_32 {
  font-size: 32rpx;
}
.fs_30 {
  font-size: 30rpx;
}
.fs_28 {
  font-size: 28rpx;
}
.fs_24 {
  font-size: 24rpx;
}
.fs_22 {
  font-size: 22rpx;
}
.color_f {
  color: #fff;
}
.color_light {
  color: #999;
}
.border_bottom {
  border-bottom: 1rpx solid rgba(153, 153, 153, .3);
}
.no_data {
  line-height: 80rpx;
  height: 80rpx;
  text-align: center;
  color: #999;
  font-size: 28rpx;
}
.y .arrived_state {
  background:linear-gradient(146deg,rgba(127,127,248,1),rgba(174,127,242,1));
  color: #fff;
}
.operation_btn {
  padding: 0 24rpx;
}
button::after{ border: none; }

.next {
  margin-top: 100rpx;
  text-align: center;
  color: #fff;
  line-height: 96rpx;
  width: 666rpx;
  height: 96rpx;
  background: linear-gradient(90deg,rgba(116,110,243,1),rgba(168,107,255,1));
  border-radius: 48rpx;
}
button[disabled] {
  background: #fff;
  color: #000;
  border: none;
}